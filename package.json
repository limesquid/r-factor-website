{
  "name": "r-factor-website",
  "version": "1.0.0",
  "main": "src/index.js",
  "homepage": "https://bitbucket.org/kamilmielnik/r-factor-website#readme",
  "repository": {
    "type": "git",
    "url": "git+https://kamilmielnik@bitbucket.org/kamilmielnik/r-factor-website.git"
  },
  "scripts": {
    "build": "better-npm-run build",
    "lint": "eslint src/",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "src/"
      }
    },
    "start": {
      "command": "react-scripts start",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "src/"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "better-npm-run": "^0.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "react-scripts": "^1.1.4"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.1.0"
  }
}
