{
  "name": "r-factor-website-frontend",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://kamilmielnik@bitbucket.org/kamilmielnik/r-factor-website.git"
  },
  "scripts": {
    "build": "better-npm-run build",
    "lint": "eslint src/",
    "start": "better-npm-run start",
    "postbuild": "react-snap"
  },
  "betterScripts": {
    "build": {
      "command": "react-scripts build",
      "env": {
        "REACT_APP_API_URL": "/api",
        "NODE_ENV": "production",
        "NODE_PATH": "src/"
      }
    },
    "start": {
      "command": "react-scripts start",
      "env": {
        "REACT_APP_API_URL": "http://127.0.0.1:8080/api",
        "NODE_ENV": "development",
        "NODE_PATH": "src/"
      }
    }
  },
  "reactSnap": {
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "better-npm-run": "^0.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "react-scripts": "^1.1.5",
    "react-snap": "^1.17.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.1",
    "codemirror": "^5.39.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-clipboard.js": "^2.0.0",
    "react-codemirror2": "^5.0.4",
    "react-cookie-consent-wrapper": "git+https://github.com/yuriyyakym/react-cookie-consent-wrapper.git",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-recaptcha": "^2.3.9",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.1.0",
    "validator": "^10.4.0"
  }
}
